<connect name="IRCCloud"
         parent="main"
         globalmax="100"
         localmax="100"
         useconnectban="no"
         useconnflood="no"
         usednsbl="no">

<connect name="IRCCloud (IPv4)"
         parent="IRCCloud"
         allow="5.254.36.56/29 192.184.8.103 192.184.9.108 192.184.9.112 192.184.10.118 192.184.10.9">


# and run make install, then uncomment this module to enable it.      #
#                                                                     #
#<module name="m_ldapoper.so">
#                                                                     #
# Configuration:                                                      #
#                                                                     #
# <ldapoper baserdn="ou=People,dc=brainbox,dc=cc"
#           server="ldap://brainwave.brainbox.cc"
#           searchscope="subtree"
#           binddn="cn=Manager,dc=brainbox,dc=cc"
